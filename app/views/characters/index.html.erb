<h1>ユニット一覧</h1>

<table class="characters-table">
  <% @characters.each do |character| %>
  <tr>
    <td>
      <%= link_to character.name, character_path(character) %>
      <img src="" alt="">
    </td>
    <td><%= character.property.name %></td>
    <td><%= character.realm.name %></td>
    <td><%= character.type.name %></td>
  </tr>
  <% end %>
</table>